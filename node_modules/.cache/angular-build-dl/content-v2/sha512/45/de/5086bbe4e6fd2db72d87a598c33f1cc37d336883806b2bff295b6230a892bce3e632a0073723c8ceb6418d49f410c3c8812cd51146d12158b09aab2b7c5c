ider = function(){\\n        var slider = $('.celula'),\\n            range = $('.custom-range'),\\n            value = $('.range-slider__value');\\n\\n        slider.each(function(){\\n\\n            value.each(function(){\\n            var value = $(this).prev().attr('value');\\n            $(this).html(value);\\n            });\\n\\n            range.on('input', function(){\\n            $(this).next(value).html(this.value);\\n            });\\n        });\\n        };\\n\\n        rangeSlider();\\n    </script> -->\\n</div>\\n\"","export default \"<input type=\\\"range\\\" class=\\\"custom-range\\\" min=\\\"0\\\" max=\\\"100\\\" step=\\\"1\\\" id=\\\"customRange3\\\" [value]=\\\"valor\\\" (input)=\\\"valor = $event.target.value\\\">\\n<span class=\\\"range-slider__value\\\"> {{ valor }} </span>\"","export default \"<!DOCTYPE html>\\n<html lang=\\\"pt-br\\\">\\n  <head>\\n    <title>ParaDecision-making Conference Nott</title>\\n\\n    <meta charset=\\\"utf-8\\\">\\n\\n    <!--pegar largura do dispositivo-->\\n    <meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1, shrink-to-fit=no\\\">\\n\\n    <!--[if IE]>\\n      <script src=\\\"https://html5shim.googlecode.com/svn/trunk/html5.js\\\">\\n      </script>\\n      <![endif]-->\\n  </head>\\n\\n\\n  <body>\\n\\n    <!-- barra de navegação -->\\n    <!-- <nav class=\\\"navbar navbar-fixed-top navbar-dark\\\">\\n        <div class=\\\"container\\\">\\n        \\n            <div class=\\\"navbar-header\\\">\\n                <a href=\\\"index.html\\\" class=\\\"navbar-brand text-dark\\\">\\n                    Meeting\\n                </a>\\n            </div>\\n\\n        \\n            <div class=\\\"collapse navbar-collapse\\\">\\n                <ul class=\\\"nav navbar-nav navbar-right\\\">\\n                    <li>\\n                        <a href=\\\"proposicao.html\\\">\\n                            Proposição\\n                        </a>\\n                    </li>\\n            